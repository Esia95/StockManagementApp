<tbody>
  <tr>
    <td><input
        class='form-control'
        type='text'
        placeholder='Artykuł'
        aria-label='default input example'
        value={{this.name}}
        {{on 'input' this.onNameChange}}
      /></td>
    <td><input
        class='form-control'
        type='text'
        placeholder='Ilość'
        aria-label='default input example'
        value={{this.count}}
        {{on 'input' this.onCountChange}}
      /></td>
    <td><input
        class='form-control'
        type='text'
        placeholder='Miejsce'
        aria-label='default input example'
        value={{this.target}}
        {{on 'input' this.onTargetChange}}
      /></td>
    <td><button
        class='btn btn-primary'
        type='submit'
        id='form-control'
        disabled={{this.isEmptyField}}
        {{on 'click' this.onSave}}
      >Dodaj</button></td>
    <td><button
        type='button'
        class='btn btn-primary'
        {{on 'click' this.clear}}
      >Usuń</button></td>
  </tr>
</tbody>